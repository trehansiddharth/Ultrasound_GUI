LoadOrderGroup = Base

;---------------------------------------------------------------;
; XP
;---------------------------------------------------------------;

[PICOPP.Dev.NT]
;CopyFiles=PICOPP.Files.Ext
AddReg=PICOPP.AddReg

[PICOPP.Dev.NT.Services]
Addservice = PICOPP, 0x00000002, PICOPP.AddService

[PICOPP.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,picopp.sys

;---------------------------------------------------------------;
; Vista and Later
;---------------------------------------------------------------;
[PICOPP.Dev.NT.6.0]
CopyFiles=PICOPP.Files.Ext
AddReg=PICOPP.AddReg

[PICOPP.Dev.NT.6.0.Services]
